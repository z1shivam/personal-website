rules_version = '2';

service firebase.storage {
  match /b/{bucket}/o {
    match /{allPaths=**} {
      allow read: if true;
      allow write: if request.auth != null && request.auth.uid == "<auth id>"; 
      allow delete: if request.auth != null && request.auth.uid  == "<auth id>";
    }
  }
}
